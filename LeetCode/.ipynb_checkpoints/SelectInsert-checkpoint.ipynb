    "def searchInsert(nums, target):\n",
    "    if target in nums:\n",
    "        return nums.index(target)\n",
    "    else:\n",
    "        i = 0\n",
    "        while target > nums[i]:\n",
    "            i+=1\n",
    "            if i >= len(nums):\n",
    "                return i\n",
    "        return i\n",
    "print(searchInsert([1,3,5,6],7))"
   ]
  }
