    "def plusOne(digits):\n",
    "    i = -1\n",
    "    digits[i] += 1\n",
    "    while digits[i] > 9:\n",
    "        print(digits)\n",
    "        digits[i] = 0\n",
    "        if i*-1 >= len(digits):\n",
    "            digits.insert(0,1)\n",
    "        else:\n",
    "            digits[i-1] += 1\n",
    "        i-=1\n",
    "    return digits\n",
    "\n",
    "print(plusOne([9,9]))"
   ]
  }
